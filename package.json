    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.15.3",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.0.3"
